public class Main {
  public static void main(String args[]) {
    int a=5;
    int b=10;
    if (a>=b){
        System.out.println("a>=b");
    }else {
        System.out.println("a<b");
    }
  }
}
 #5 
public class Main {
    public static void main(String[] args) {
        System.out.println(isSumInRange(5, 10));
    }
    public static boolean isSumInRange(int a, int b) {
        int sum = a + b;
        return sum >= 10 && sum <= 20;
    }
}

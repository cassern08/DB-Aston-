public class Main {
 public static void main (String[] args) {
  String a1 = "Hello";
  String a2 = "World";
  String a3 = a1+a2;
  System.out.println(a3);
  String b1 = "Число 10: ";
  int b2 = 10;
  String b3 = b1 + b2;
  System.out.println(b3);
  String c1 = "Home";
  String c2 = "[" + c1 + "] = " + 1;
  System.out.println(c2);
  }
}
